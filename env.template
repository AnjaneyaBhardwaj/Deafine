# ElevenLabs API Key (REQUIRED)
ELEVEN_API_KEY=your_elevenlabs_api_key_here

# Audio Settings
DEAFINE_CHUNK_MS=320
DEAFINE_HOP_MS=160

# Voice Activity Detection (optional - only if webrtcvad installed)
# Set to false to disable VAD and send all audio
DEAFINE_USE_VAD=true
# VAD aggressiveness: 0 (least) to 3 (most aggressive at filtering silence)
DEAFINE_VAD_AGGRESSIVENESS=2

# ElevenLabs Settings
DEAFINE_ELEVENLABS_CHUNK_SECS=5
